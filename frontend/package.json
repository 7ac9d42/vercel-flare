{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node scripts/log-env.js",
    "dev": "next dev",
    "icons:optimize": "svgo -f public/icons",
    "build": "node scripts/fetch-icons.js && npm run icons:optimize && next build",
    "start": "serve out --listen 5005 --single --no-clipboard",
    "lint": "eslint . --ext .ts,.tsx,.js --max-warnings=0"
  },
  "dependencies": {
    "js-yaml": "^4.1.1",
    "next": "16.0.8",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.8",
    "serve": "^14.2.3",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3"
  }
}
